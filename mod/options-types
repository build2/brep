#ifndef MOD_OPTIONS_TYPES
#define MOD_OPTIONS_TYPES

#include <brep/types>
#include <brep/utility>

namespace brep
{
  // brep types
  //
  enum class page_form
  {
    full,
    brief
  };

  struct page_menu
  {
    string label;
    string link;

    page_menu () = default;
    page_menu (string b, string l): label (move (b)), link (move (l)) {}
  };

}

#endif // MOD_OPTIONS_TYPES
