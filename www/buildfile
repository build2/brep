define css: file

css{*}:
{
  extension = css
  install = data/www/
}

define scss: file

scss{*}:
{
  extension = scss
  install = data/www/
}

define xhtml: file

xhtml{*}:
{
  extension = xhtml
  install = data/www/
}

./: css{*} scss{*} xhtml{*}
